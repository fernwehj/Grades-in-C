#include <stdio.h>
int main()
{
   int mark;
   printf("input the mark : ");
   scanf("%d", &mark);
   if (mark >=85)
   {
	printf("Grade A");
   }
   else if (mark >= 70)
   {
	printf("Grade B");
   }
   else if (mark >= 55)
   {
	printf("Grade C");
   }
   else if (mark >= 40)
   
   {
	printf("Grade D");
   }
   else
   {
	printf("Grade F");
   }
   return 0;
}
